<form action="/addSales" method="post">
Product Name
<input type="text" name="ProductName" id="" value="<%=rows.productName%>">
Price
<input type="text" name="Price" id="Text2" value="<%=rows.price%>">
Quantity
<input type="number" name="Qty" id="Text1" onchange="multiply()">
Total
<input type="text" name="Total" id="txtresult">
<input type="submit" value="submit">
</form>
<script>
function multiply() {

var first_number = parseInt(document.getElementById("Text1").value);
var second_number = parseInt(document.getElementById("Text2").value);
var result = first_number * second_number;

document.getElementById("txtresult").value = result;
}
</script>