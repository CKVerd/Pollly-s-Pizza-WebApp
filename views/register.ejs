<html>
    <head>
        <title>Login Page</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- FONTS -->
        <link href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

        <!-- JS -->
        <script src="js/login.js"></script>

        <!-- CSS -->
        <link rel="stylesheet" type="text/css" href="css/login.css">

    </head>
    <body>
        <div class = "main">
            <div class ="inner">
                <div class = "login-div">
                    <form class = "form-1" method="post" action="/new">
                        <input type = "text" class = "input uname" id = "username"  name="username" placeholder = "username">
                        <input type = "password" class = "input pword" id = "password" name="password" placeholder = "password">
                        <input type = "text" class = "input uname" id = "password" name="secq1" placeholder = "secq1">
                        <input type = "text" class = "input uname" id = "password" name="secq2" placeholder = "secq2">
                        <input type = "text" class = "input uname" id = "password" name="secq3" placeholder = "secq3">
                        <input type = "text" class = "input uname" id = "password" name="seca1" placeholder = "seca1">
                        <input type = "text" class = "input uname" id = "password" name="seca2" placeholder = "seca2">
                        <input type = "text" class = "input uname" id = "password" name="seca3" placeholder = "seca3">
                        <input type = "submit" class = "submit-button" id = "login-submit" value = "Register">
                    </form>
                </div>
                <a href = "/forgot-pass" class = "forgot">Forgot Password</a>
            </div>
        </div>
    </body>
</html>
